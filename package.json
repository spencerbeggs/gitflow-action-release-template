		"eslint-plugin-prettier": "^3.1.4",
		"graphql-tag": "^2.11.0",
		"husky": "^4.3.0",
		"inquirer": "^7.3.3",
		"jest": "^26.4.2",
		"lint-staged": "^10.4.0",
